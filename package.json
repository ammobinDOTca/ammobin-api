    "dev": "ts-node-dev ./src/api/index.ts",
    "dev:worker": "ts-node-dev ./src/worker/index.ts"
  },
  "author": "Simon Ramsay",
  "license": "MIT",
  "dependencies": {
    "ammobin-classifier": "0.0.29",
    "apollo-server": "^2.2.6",
    "apollo-server-hapi": "^2.2.6",
    "axios": "^0.18.0",
    "boom": "^7.2.2",
    "catbox-redis": "^4.2.4",
    "cheerio": "^1.0.0-rc.2",
    "delay": "^4.1.0",
    "fluent-logger": "^3.2.1",
    "graphql": "^14.0.2",
    "hapi": "^17.7.0",
    "hapi-cron": "^1.0.4",
    "influx": "^5.0.7",
    "jsonp": "^0.2.1",
    "moment": "^2.22.2",
    "redis": "^2.7.1",
    "rsmq-worker": "^0.5.2",
    "throat": "^4.1.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "husky": "^1.2.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
