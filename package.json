{
  "name": "ammobin-api",
  "version": "2.0.0",
  "description": "ammobin.ca api",
  "main": "./dist/api/index.js",
  "scripts": {
    "clean": "rm -rf dist lambda",
    "start": "node ./dist/api/api.js",
    "start:worker": "node --tls-min-v1.0 ./dist/worker/redis-worker.js",
    "start:refresher": "node ./dist/refresher/redis-refresher.js",
    "test:lint": "tslint ./src/**/*.ts",
    "test": "npm run test:lint && npm run build",
    "format": "pretty-quick",
    "build": "tsc",
    "build-lambda": "webpack  && cp graphql.gql lambda/graphql",
    "dev": "DEV=true ts-node-dev ./src/api/api.ts",
    "dev-us": "REGION=US DEV=true ts-node-dev ./src/api/api.ts",
    "dev:worker": "DEV=true ts-node-dev ./src/worker/redis-worker.ts",
    "dev:refresher": "DEV=true ts-node-dev ./src/refresher/redis-refresher.ts",
    "generate-types": "gql2ts graphql.gql > src/graphql-types.ts && sh export-graphql-types.sh",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "types": "dist/index.d.ts",
  "author": "Simon Ramsay",
  "license": "MIT",
  "dependencies": {
    "ammobin-classifier": "0.1.2",
    "apollo-server": "^2.21.0",
    "apollo-server-cache-redis": "^1.2.3",
    "apollo-server-hapi": "^2.21.0",
    "apollo-server-lambda": "^2.21.0",
    "apollo-server-plugin-response-cache": "^0.3.11",
    "aws-sdk": "^2.849.0",
    "axios": "^0.21.1",
    "boom": "^7.3.0",
    "cheerio": "^1.0.0-rc.5",
    "chrome-aws-lambda": "^5.3",
    "delay": "^4.4.1",
    "fluent-logger": "^3.4.1",
    "graphql": "^14.7.0",
    "hapi": "^18.1.0",
    "hapi-lambda": "^1.0.0",
    "jsonp": "^0.2.1",
    "moment": "^2.29.1",
    "node-cron": "^2.0.3",
    "puppeteer-core": "^5.3",
    "redis": "^3.1.1",
    "rsmq-worker": "^0.5.2",
    "snyk": "^1.465.0",
    "throat": "^5.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/boom": "^7.3.0",
    "@types/graphql": "^14.5.0",
    "@types/hapi": "^18.0.5",
    "@types/moment": "^2.13.0",
    "@types/puppeteer": "^2.1.5",
    "copy-webpack-plugin": "^5.1.2",
    "gql2ts": "^1.10.1",
    "husky": "^4.3.8",
    "json-loader": "^0.5.7",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^2.0.2",
    "ts-loader": "^6.2.2",
    "ts-node-dev": "^1.1.1",
    "tslint": "^5.20.1",
    "typescript": "^3.9.9",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": " pretty-quick --staged && npm t"
    }
  },
  "snyk": true
}
