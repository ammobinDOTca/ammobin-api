{
  "name": "ammobin-api",
  "version": "1.0.0",
  "description": "ammobin.ca api",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/api/index.js",
    "start:worker": "node ./dist/worker/index.js",
    "start:refresher": "node ./dist/refresher/index.js",
    "test:lint": "tslint ./src/**/*.ts",
    "test": "npm run test:lint && npm run build",
    "format": "pretty-quick",
    "build": "tsc",
    "dev": "ts-node-dev ./src/api/index.ts",
    "dev:worker": "ts-node-dev ./src/worker/index.ts",
    "dev:refresher": "ts-node-dev ./src/refresher/index.ts",
    "generate-types": "gql2ts graphql.gql > src/graphql-types.ts && sh export-graphql-types.sh",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "types": "dist/index.d.ts",
  "author": "Simon Ramsay",
  "license": "MIT",
  "dependencies": {
    "ammobin-classifier": "0.0.35",
    "apollo-server": "^2.5.0",
    "apollo-server-hapi": "^2.5.0",
    "axios": "^0.18.0",
    "boom": "^7.3.0",
    "cheerio": "^1.0.0-rc.3",
    "delay": "^4.2.0",
    "fluent-logger": "^3.3.1",
    "graphql": "^14.3.0",
    "hapi": "^18.1.0",
    "jsonp": "^0.2.1",
    "moment": "^2.24.0",
    "node-cron": "^2.0.3",
    "redis": "^2.7.1",
    "rsmq-worker": "^0.5.2",
    "throat": "^4.1.0",
    "winston": "^3.2.1",
    "snyk": "^1.161.1"
  },
  "devDependencies": {
    "gql2ts": "^1.10.1",
    "husky": "^2.3.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.10.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": " pretty-quick --staged && npm t"
    }
  },
  "snyk": true
}
